# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(language "english")
(default-image
    (width 3840)
    (height 2160)
    (unit pixels)
    (xresolution 300)
    (yresolution 300)
    (resolution-unit inches)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type background)
    (comment "sketch"))
(default-grid
    (style solid)
    (fgcolor (color-rgba 1 1 1 1))
    (bgcolor (color-rgba 0 0 0 1))
    (xspacing 10)
    (yspacing 10)
    (spacing-unit inches)
    (xoffset 0)
    (yoffset 0)
    (offset-unit inches))
(layer-previews no)
(group-layer-previews no)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(check-updates no)
(monitor-xresolution 142)
(monitor-yresolution 142)
(default-view
    (show-menubar no)
    (show-statusbar no)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection no)
    (show-layer-boundary no)
    (show-canvas-boundary no)
    (show-guides no)
    (show-grid no)
    (show-sample-points no)
    (snap-to-guides no)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(default-fullscreen-view
    (show-menubar no)
    (show-statusbar no)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection no)
    (show-layer-boundary no)
    (show-canvas-boundary no)
    (show-guides no)
    (show-grid no)
    (show-sample-points no)
    (snap-to-guides no)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(image-resize-fill-type background)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
